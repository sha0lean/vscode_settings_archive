  "better-comments.tags": [
    // {
    //   // commentary
    //   "tag": "",
    //   "color": "#007a64",
    //   "strikethrough": false,
    //   "underline": false,
    //   "backgroundColor": "#transparent",
    //   "bold": false,
    //   "italic": false
    // },
    {
      //! commentary
      "tag": "!",
      "color": "#d1c000",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#000000",
      "bold": false,
      "italic": true
    },
    {
      //? commentary
      "tag": "?",
      "color": "#545454",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#000000",
      "bold": false,
      "italic": false
    },
    {
      //- commentary
      "tag": "-",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": true,
      "italic": false
    },
    {
      //: commentary
      "tag": ":",
      "color": "#000000",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#474747",
      "bold": true,
      "italic": true
    },
    {
      //= commentary
      "tag": "=",
      "color": "#363636",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#1a1a1a",
      "bold": true,
      "italic": false
    }

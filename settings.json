  "terminal.integrated.fontSize": 13,
  //! term osx ------------------------------------------------------------------------------------------------------------------
  "terminal.integrated.profiles.osx": {
    "Bash": {
      "path": "/bin/bash"
    },
    "iTerm2": {
      "path": "iTerm.app"
    },
    "Terminal": {
      "path": "Terminal.app"
    },
    "Zsh": {
      "path": "/bin/zsh"
    }
  },
  //! term windows --------------------------------------------------------------------------------------------------------------
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",

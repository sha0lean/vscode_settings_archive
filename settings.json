{
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnSave": true // Activer le formatage automatique √† chaque enregistrement
  },
  //! better comments -----------------------------------------------------------------------------------------------------------
  "better-comments.highlightPlainText": true,
  "better-comments.multilineComments": true,
  "better-comments.tags": [
    {
      "backgroundColor": "#000000",
      "bold": false,
      "color": "#d1c000",
      "italic": true,
      "strikethrough": false,
      "tag": "!",
      "underline": false
    },
    {
      //? commentary
      "backgroundColor": "#000000",
      "bold": false,
      "color": "#545454",
      "italic": false,
      "strikethrough": false,
      "tag": "?",
      "underline": false
    },
    {
      //- commentary
      "backgroundColor": "transparent",
      "bold": true,
      "color": "#474747",
      "italic": false,
      "strikethrough": true,
      "tag": "-",
      "underline": false
    },
    {
      //::: commentary
      "backgroundColor": "#474747",
      "bold": true,
      "color": "#000000",
      "italic": true,
      "strikethrough": false,
      "tag": ":::",
      "underline": false
    },
    {
      //= commentary
      "backgroundColor": "#1a1a1a",
      "bold": true,
      "color": "#363636",
      "italic": false,
      "strikethrough": false,
      "tag": "=",
      "underline": false
    }
  ],
  "color-highlight.markerType": "dot-after",
  //! comment box ---------------------------------------------------------------------------------------------------------------
  "commentBox.styles": {
    "defaultStyle": {
      "bottomEdgeToken": "‚ïê",
      "bottomLeftToken": "‚ïö",
      "capitalize": false,
      "commentEndToken": "‚ïê‚ïù",
      "commentStartToken": "‚ïî",
      "extendSelection": true,
      "ignoreInnerIndentation": false,
      "ignoreOuterIndentation": false,
      "leftEdgeToken": "‚ïë ",
      "removeEmptyLines": false,
      "rightEdgeToken": " ‚ïë",
      "textAlignment": "left",
      "topEdgeToken": "‚ïê",
      "topRightToken": "‚ïê‚ïó"
      //  ‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó
      //  ‚ïë             ‚ïë
      //  ‚ïë             ‚ïë
      //  ‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù
    }
  },
  //! editor --------------------------------------------------------------------------------------------------------------------
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortImports": "explicit"
  },
  "editor.codeLensFontFamily": "Fira Code, FiraCode Nerd Font Mono",
  "editor.cursorBlinking": "blink",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.cursorMoveOnType": false,
  "editor.fontFamily": "Fira Code, FiraCode Nerd Font Mono",
  "editor.fontLigatures": "'cv01', 'cv02', 'cv10', 'ss02', 'cv11', 'cv14', 'ss05','ss03','ss09','cv25','cv26','cv32','cv27','ss06','ss07'",
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.guides.bracketPairs": true,
  "editor.inlayHints.padding": true,
  "editor.insertSpaces": false,
  "editor.largeFileOptimizations": false,
  "editor.lineHeight": 0,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fit",
  "editor.mouseWheelScrollSensitivity": 0.3,
  "editor.renderWhitespace": "trailing",
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.suggest.insertMode": "replace",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "emmet.triggerExpansionOnTab": true,
  "explorer.compactFolders": false,
  //! files ---------------------------------------------------------------------------------------------------------------------
  "files.associations": {
    "*.cfg": "cuda-cpp",
    "*.css": "tailwindcss",
    "*.toml": "csharp",
    "*.txt": "csharp"
  },
  "files.defaultLanguage": "markdown",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": false,
    "**/.project": true,
    "**/.settings": true
  },
  //! git -----------------------------------------------------------------------------------------------------------------------
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.advanced": {
    "language": "fr"
  },
  "gitlens.blame.avatars": false,
  "gitlens.blame.format": "${message|30?} ${agoOrDate|20-}",
  "gitlens.blame.highlight.locations": ["gutter", "line", "overview"],
  "gitlens.changes.locations": ["gutter", "line", "overview"],
  "gitlens.changes.toggleMode": "window",
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.command": "gitlens.toggleFileHeatmap",
  "gitlens.codeLens.scopes": ["document"],
  "gitlens.currentLine.format": "${agoOrDate}${' via 'pullRequest}${ | message|50?}",
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.currentLine.uncommittedChangesFormat": "------------------------   ${ago} üìù",
  "gitlens.graph.dimMergeCommits": true,
  "gitlens.graph.layout": "editor",
  "gitlens.graph.minimap.additionalTypes": ["tags"],
  "gitlens.graph.minimap.enabled": false,
  "gitlens.graph.showRemoteNames": true,
  "gitlens.heatmap.locations": ["gutter"],
  "gitlens.heatmap.toggleMode": "window",
  "gitlens.hovers.annotations.details": false,
  "gitlens.hovers.currentLine.details": false,
  "gitlens.statusBar.alignment": "left",
  "gitlens.statusBar.format": "${author}   |   ${agoOrDate}   ${'   |   via 'pullRequest}",
  "gitlens.views.commitDetails.files.layout": "list",
  "gitlens.views.commits.avatars": false,
  "gitlens.views.commits.showBranchComparison": false,
  "gitlens.views.formats.commits.description": "${ agoOrDate}",
  "gitlens.views.formats.commits.label": "${‚ù∞ tips ‚ù±  }${message}",
  //! gitmoji -------------------------------------------------------------------------------------------------------------------
  "gitmoji.addCustomEmoji": [],
  "gitmoji.showEmojiCode": true,
  //! javascript ----------------------------------------------------------------------------------------------------------------
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "javascript.suggest.paths": false,
  //! json ----------------------------------------------------------------------------------------------------------------------
  "json.maxItemsComputed": 8000,
  //! material icon -------------------------------------------------------------------------------------------------------------
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.files.color": "#26a69a",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.opacity": 0.9,
  "material-icon-theme.saturation": 1,
  //! npm -----------------------------------------------------------------------------------------------------------------------
  "npm-intellisense.scanDevDependencies": true,
  //! paths ---------------------------------------------------------------------------------------------------------------------
  "path-intellisense.absolutePathToWorkspace": true,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "path-intellisense.extensionOnImport": false,
  "path-intellisense.mappings": {
    "/": "${workspaceFolder}",
    "global": "/Users/shao/globalLibs",
    "lib": "${workspaceFolder}/lib"
  },
  "path-intellisense.showHiddenFiles": false,
  //! project manager -----------------------------------------------------------------------------------------------------------
  "projectManager.git.baseFolders": [
    "C:\\Users\\shao\\workspace",
    "C:\\Users\\shao\\AppData\\Roaming\\Code\\User",
    "/Users/shao/workspace"
  ],
  "projectManager.sortList": "Name",
  "search.followSymlinks": false,
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.keybindingsPerPlatform": true,
  //! status --------------------------------------------------------------------------------------------------------------------
  "statusbarerror.color.error": "#ffd0d0",
  "statusbarerror.color.hint": "#ffee00",
  "statusbarerror.color.info": "#d0e4ff",
  "statusbarerror.icon.error": "‚ùï",
  "statusbarerror.icon.hint": "üí°",
  "statusbarerror.icon.info": "‚è≠",
  "statusbarerror.icon.warning": "‚ùî",
  "statusbarerror.wholeLine.errorColor": "#363636",
  "statusbarerror.wholeLine.errorFontColor": "#777777",
  "statusbarerror.wholeLine.show": true,
  "statusbarerror.wholeLine.warningColor": "#ff980090",
  "statusbarerror.wholeLine.warningFontColor": "#efefef",
  //! tailwind ------------------------------------------------------------------------------------------------------------------
  "tailwind-fold.autoFold": true,
  "tailwind-fold.foldedText": " tailwind ",
  "tailwind-fold.foldedTextBackgroundColor": "#52b7ee09",
  "tailwind-fold.foldedTextColor": "#7cdbfe7e",
  "tailwind-fold.showTailwindImage": true,
  "tailwind-fold.supportedLanguages": [
    "html",
    "typescriptreact",
    "javascriptreact",
    "typescript",
    "javascript",
    "vue-html",
    "vue",
    "php",
    "markdown",
    "coffeescript",
    "svelte",
    "astro"
  ],
  "tailwind-fold.unfoldedTextOpacity": 0.6,
  "tailwind-fold.unfoldIfLineSelected": true,
